    </valuepage>

    <valuepage id="preIllum" rows="4" cols="5.8">
      <values>
	0 ms, 1 ms, 2 ms, 5 ms, 10 ms, 20 ms, 50 ms, 100 ms, 200 ms, 500 ms
      </values>
      <customvalue caption="Pre-illumination time"/>
    </valuepage>

    <valuepage id="postIllum" rows="4" cols="5.8">
      <values>
	0 ms, 1 ms, 2 ms, 5 ms, 10 ms, 20 ms, 50 ms, 100 ms, 200 ms, 500 ms
      </values>
      <customvalue caption="Post-illumination time"/>
    </valuepage>

    <valuepage id="marginPreCCD" rows="4" cols="5.8">
      <values>
	0 ms, 1 ms, 2 ms, 5 ms, 10 ms, 20 ms, 50 ms, 100 ms, 200 ms, 500 ms
      </values>
      <customvalue caption="Pre-CCD avoidance time"/>
    </valuepage>

    <valuepage id="marginPostCCD" rows="4" cols="5.8">
      <values>
	0 ms, 1 ms, 2 ms, 5 ms, 10 ms, 20 ms, 50 ms, 100 ms, 200 ms, 500 ms
      </values>
      <customvalue caption="Post-CCD avoidance time"/>
    </valuepage>

    <valuepage id="delay"  cols="5.8">
      <values>
	100 ms, 150 ms, 200 ms, 300 ms, 500 ms, 700 ms,
	1 s, 1.5 s, 2 s, 3 s, 5 s, 7 s
	10 s, 15 s, 20 s, 30 s, 45 s, 60 s, 90 s, 120 s, 180 s
      </values>
      <customvalue caption="Delay before video stimulation"/>
    </valuepage>

    <valuepage id="dur"  cols="5.8">
      <values>
	100 ms, 150 ms, 200 ms, 300 ms, 500 ms, 700 ms,
	1 s, 1.5 s, 2 s, 3 s, 5 s, 7 s
	10 s, 15 s, 20 s, 30 s, 45 s, 60 s, 90 s, 120 s, 180 s
      </values>
      <customvalue  caption="Duration of video stimulation"/>
    </valuepage>

    <valuepage id="prog"  cols="4.8">
      <values>
	01, 02, 03, 04, 05, 11, 12, 13, 14, 15, 21, 22, 23, 24, 25,
      </values>
    </valuepage>

    <valuepage id="parA"  cols="5.8">
      <values>
	01, 02, 03, 04, 05, 11, 12, 13, 14, 15,
	21, 22, 23, 24, 25, 31, 32, 33, 34,
      </values>
      <customvalue  caption="Parameter value"/>
    </valuepage>

    <valuepage id="parB"  cols="4">
      <values>
	01, 02, 03, 04, 05, 11, 12, 13, 14, 15,
	21, 22, 23, 24, 25, 31, 32, 33, 34,
      </values>
      <customvalue  caption="Parameter value"/>
    </valuepage>

    <valuepage id="parC"  cols="4">
      <values>
	01, 02, 03, 04, 05, 11, 12, 13, 14, 15,
	21, 22, 23, 24, 25, 31, 32, 33, 34,
      </values>
      <customvalue  caption="Parameter value"/>
    </valuepage>
  </page> <!-- acqCCD -->

  <!-- %%%%%%%%%%%%%%%%%%%%% ACQUISITION PAGE %%%%%%%%%%%%%%%%%% -->
  <page id="acquisition" subdxr="4.1">
    <param id="exptname" label="Expt. Name: %1" x="3.1" y="0"
	    caption="Experiment Name" bg="#88aaff"/>
    <param id="trialno" label="Trial No.: %1" caption="Trial Number"
	    bg="#77bbff"/>
    <action id="load"  label="Load Old Data..." y="3" bg="#88ffff"/>
    
    <action id="singleframe" label="Single Frame" x="4.1" y="0" bg="#ddbbff"/>
    <action id="singletrial" label="Single Trial" bg="#bb99ff"/>
    <param id="dummy" label="Dummy"  bg="#ffffff"/>
    <action id="log"  label="Log Note..." bg="#eeeecc"/>
    <param id="contEphys" label="Cont. E'phys Disabled" x="2.1" y="1"
	     bg="#22ff77"/>
    <param id="autoRun"  label="Auto Run Disabled"  bg="#9977ff"/>
    <group id="a" x="1.1" y="0">
      <param id="trialPeriod" label="Trial period" bg="#44ffaa" x="1" y="3"/>
      <arraycontrol id="camvals" x="0" y="0">
	<auto id="camvals" enum="CAMERAS"/>
      </arraycontrol>
    </group>

    <label id="walltime" label="%1" x="1.1" y="1" bg="#99aa99"/>
    <label id="exptelapsed" label="Since Start: %1" bg="#88bb88"/>
    <label id="trialelapsed" label="Since Last: %1" bg="#77cc77"/>

    <arraypage id="camvals">
      <action id="blackPlus" label="More black"/>
      <action id="blackMin" label="Less black"/>
      <action id="whitePlus" label="More white"/>
      <action id="whvaluein" label="Less white"/>
      <action id="reset" label="Reset"/>
    </arraypage>

    <valuepage id="trialPeriod"  cols="1">
      <value value="1 min"/>
      <value value="2 min"/>
      <value value="5 min"/>
      <value value="10 min"/>
      <customvalue caption="Trial period"/>
    </valuepage>

  </page> <!-- acquisition -->

  <!-- %%%%%%%%%%%%%%%%%%%%% ANALYSIS PAGE %%%%%%%%%%%%%%%%%% -->
