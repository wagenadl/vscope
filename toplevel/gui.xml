      <radio id="zoom" label="Zoom" x="4.1" y="0" bg="#88ffcc"/>
      <radio id="roiMove" label="Move ROI" x="3.1" y="0" bg="#aaff00"/>
      <radio id="roiRotSize" label="Resize ROI" bg="#88ff44"/>
      <radio id="roiRotate" label="Rotate ROI" bg="#11ff11"/>
      <radio id="roiSelect" label="Select ROI" x="2.1" y="0" bg="#aaeeaa"/>
      <radio id="roiBlob" label="Blob ROI" bg="#ffff88"/>
      <radio id="roiAdd" label="Create Elliptic ROI" bg="#ffff00"/>
    </group>
    <action id="roiDelete" label="Delete ROI" x="4.1" y="3" bg="#ff8833"/>
