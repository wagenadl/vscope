      <auto enum="DIGILINES"/>
    </valuepage>

    <valuepage id="refTrace" rows="4" cols="4">
      <auto enum="AICHAN"/>
    </valuepage>

    <valuepage id="showROIs"  cols="1">
      <auto enum="SHOWROIS"/>
    </valuepage>

    <valuepage id="debleach" rows="3" cols="1">
      <auto enum="DEBLEACH"/>
    </valuepage>
  </page> <!-- analysis -->

  <!-- %%%%%%%%%%%%%%%%%%%%% MAINTENANCE PAGE %%%%%%%%%%%%%%%%%% -->
  <page id="maintenance" subdxr="1.1">
    <group id="chk" x="4.1" spancols="1.">
      <pageopen id="focus" label="Focus" bg="#ffcc88"/>
      <pageopen id="liveEphys" label="Oscilloscope" bg="#eeaaaa"/>
      
      <pageopen id="checkDaq" label="DAQ Board Status" bg="#bbffff"/>
      <pageopen id="checkCam" label="Camera Status" bg="#ccccff"/>
    </group>
    <page id="focus" subdxr="1.05" subdxl="2.05">
      <action id="autoRange" label="Auto Range" x="0.5" y="4" bg="#ddcc88"/>
      <group id="a" default="mode" x="3" y="0" spancols="1" spanrows="4">
        <param id="mode" label="View mode" bg="#eecc77"/>
        <param id="camA" label="Camera A"  bg="#bbddff"/>
        <param id="camB" label="Camera B"  bg="#ffff77"/>
      </group>
      <group id="val" x="0" y="0" spancols="2" spanrows="4">
        <label id="focusA" label="A Focus" bg="#cceeff"/>
        <label id="focusB" label="B Focus" bg="#ffff77"/>
        <label id="intensityA" label="A Avg/Max" bg="#bbddff"/>
        <label id="intensityB" label="B Avg/Max" bg="#eeee55"/>
        <label id="alignScale" label="Scale"  bg="#ffffaa"/>
        <label id="alignRotate" label="Rotation" bg="#ffddcc"/>
        <label id="alignX" label="X Shift" bg="#ffdd99"/>
        <label id="alignY" label="Y Shift" bg="#ffddbb"/>
      </group>
      <valuepage id="camA" cols="1" rows="5">
        <auto enum="CAMERAS"/>
      </valuepage>
      <valuepage id="camB" cols="1" rows="5">
        <auto enum="CAMERAS"/>
      </valuepage>
      <valuepage id="mode" cols="1" rows="5">
        <auto enum="FOCUSMODE"/>
      </valuepage>
    </page> <!-- focus -->

    <page id="liveEphys" subdxr="1.1">
      <group id="a" default="timeBase" x="3" colspan="1">
