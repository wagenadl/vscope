      <param id="refDigi" label="Digi. Ref." x="3.1" y="4" bg="#55ccdd"
	      hidewhendisabled="yes"/>
      <param id="refFreq" label="Ref. Freq." x="3.1" y="4" bg="#55ccdd"
	      hidewhendisabled="yes"/>
      <param id="showROIs" label="ROIs" x="2.1" y="3" bg="#66eedd"/>
      <param id="debleach" label="Debleach" x="2.1" y="4" bg="#66ddff"/>
    </group>
    <action id="roiDeleteAll" label="Delete All ROIs" x="4.1" y="4"
	    bg="#ff4411"/>
    <action id="zoomIn" label="Zoom In" x="4.1" y="1" bg="#55eecc"/>
    <action id="zoomOut" label="Zoom Out" bg="#22ddcc"/>

    <valuepage id="refType" rows="3" cols="1">
      <auto enum="REFTYPE"/>
    </valuepage>

    <valuepage id="refFreq"  cols="4.8">
      <value value="0.1 Hz"/>
      <value value="0.2 Hz"/>
      <value value="0.3 Hz"/>
      <value value="0.5 Hz"/>
      <value value="0.7 Hz"/>
      
      <value value="1 Hz"/>
      <value value="2 Hz"/>
      <value value="3 Hz"/>
      <value value="5 Hz"/>
      <value value="7 Hz"/>
      
      <value value="10 Hz"/>
      <value value="15 Hz"/>
      <value value="20 Hz"/>
